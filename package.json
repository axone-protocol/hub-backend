{
  "name": "hub-backend",
  "version": "0.1.0",
  "description": "Axone Hub Backend",
  "homepage": "https://axone.xyz",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Axone",
    "web": "https://axone.xyz"
  },
  "private": true,
  "scripts": {
    "build": "prisma generate && nest build",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "start": "nest start",
    "dev": "npm run prisma:init && dotenvx run --env-file=.env.local -- nest start --watch --debug",
    "prisma:init": "npx prisma generate && npx dotenvx run --env-file=.env.local -- prisma migrate deploy",
    "upgrade": "ncu -u && npm i"
  },
  "engines": {
    "node": "^18.20.0",
    "yarn": "~1.22.19"
  },
  "dependencies": {
    "@cosmjs/crypto": "^0.32.3",
    "@cosmjs/encoding": "^0.32.3",
    "@dotenvx/dotenvx": "^0.35.1",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^4.0.2",
    "@prisma/client": "^5.13.0",
    "big.js": "^6.2.1",
    "cache-manager": "^5.5.2",
    "colorette": "^2.0.20",
    "ioredis": "^5.4.1",
    "joi": "^17.11.0",
    "pg": "^8.11.4",
    "prisma": "^5.12.1",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@types/big.js": "^6.2.2",
    "@types/ioredis": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.7",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "eslint": "^9.1.1",
    "globals": "^15.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
